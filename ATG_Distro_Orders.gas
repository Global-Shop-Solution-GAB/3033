Program.Sub.Part_Fill.Start
V.Local.sSQL.Declare(String)
V.Local.sSQL2.Declare(String)
V.Local.sPart.Declare(String)
V.Local.sKey.Declare(String)
V.Local.fQty.Declare(Float)
V.Local.sJob.Declare(String)
V.Local.sOrder.Declare(String)
V.Local.sSuf.Declare(String)

'get job
V.Local.sJob.Set(V.Args.sJob)
Gui.F_Material.txtWO.Text(V.Local.sJob)
'get suffix
V.Local.sSuf.Set(V.Args.sSuf)
Gui.F_Material.txtWOS.Text(V.Local.sSuf)
'get order
V.Local.sOrder.Set(V.Args.sOrder)
Gui.F_Material.txtPSO.Text(V.Local.sOrder.String)
'get part
V.Local.sPart.Set(V.Args.sPart)
Gui.F_Material.txtBaseP.Text(V.Local.sPart)
Gui.F_Material.txtSBin.Text("SSS")
Gui.F_Material.txtDt.Text(V.Ambient.Date)
Gui.F_Material.cmdOK.Enabled(false)
Gui.F_Material.lvwParts.MultiSelect(true)
Gui.F_Material.lvwParts.ClearItems

'Get list of inventory parts based on Master Part
F.Intrinsic.String.Concat("Select * from V_Inventory_Mstr where Part like '",V.Local.sPart,"%' order by Part desc",V.Local.sSQL)
'F.Intrinsic.UI.Msgbox(V.Local.sSQL)
F.ODBC.Connection!con.OpenRecordsetRO("rstInv",V.Local.sSQL)
'F.Intrinsic.Control.If(V.ODBC.con!rstWO.EOF,=,True)
'	F.Intrinsic.UI.Msgbox("Empty")
'F.Intrinsic.Control.EndIf
F.Intrinsic.Control.DoUntil(V.ODBC.con!rstInv.EOF,=,True)
	V.Local.sKey.Set(V.ODBC.con!rstInv.FieldVal!Part)
	F.Intrinsic.Control.If(V.ODBC.con!rstInv.FieldVal!UM_Inventory,=,"EA")
		F.Intrinsic.Math.Mult(V.ODBC.con!rstInv.FieldVal!Qty_OnHand,V.ODBC.con!rstInv.FieldVal!Length_Raw_Matl,V.Local.fQty)
	F.Intrinsic.Control.Else
		V.Local.fQty.Set(V.ODBC.con!rstInv.FieldValFloat!Qty_Onhand)
	F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.If(V.ODBC.con!rstInv.FieldValIsNull!Bin,=,False)
		F.Intrinsic.String.Concat("Select BIN_DESCRIPTION from V_BIN_Master where Bin='",V.ODBC.con!rstInv.FieldVal!Bin,"'",V.Local.sSQL)
		Gui.F_Material.lvwParts.AddListItem(V.Local.sKey,V.ODBC.con!rstInv.FieldVal!Bin)
		F.ODBC.Connection!con.OpenRecordsetRO("rstBin",V.Local.sSQL)
		F.Intrinsic.Control.If(V.ODBC.con!rstBin.FieldValIsNull!BIN_Description,=,False)
			Gui.F_Material.lvwParts.SetListItemSubItemText(V.Local.sKey,1,V.ODBC.con!rstBin.FieldVal!BIN_Description)
		F.Intrinsic.Control.else
			Gui.F_Material.lvwParts.SetListItemSubItemText(V.Local.sKey,1,"")
		F.Intrinsic.Control.EndIf
		F.ODBC.con!rstBin.Close
	F.Intrinsic.Control.Else
		Gui.F_Material.lvwParts.AddListItem(V.Local.sKey,"No Bin")
	F.Intrinsic.Control.EndIf
	Gui.F_Material.lvwParts.SetListItemSubItemText(V.Local.sKey,2,V.ODBC.con!rstInv.FieldVal!Part)
	'Gui.F_Material.lvwParts.SetListItemSubItemText(V.Local.sKey,3,V.ODBC.con!rstInv.FieldVal!Description)
	'get Lot Number
	F.Intrinsic.String.Concat("Select Lot from V_Item_Master where Part='",V.Local.sKey,"'",V.Local.sSQL)
	F.ODBC.Connection!con.OpenRecordsetRO("rstLot",V.Local.sSQL)
	F.Intrinsic.Control.If(V.ODBC.con!rstLot.EOF,=,False)
		Gui.F_Material.lvwParts.SetListItemSubItemText(V.Local.sKey,3,V.ODBC.con!rstLot.FieldVal!Lot)
	F.Intrinsic.Control.Else
		Gui.F_Material.lvwParts.SetListItemSubItemText(V.Local.sKey,3,"")
	F.Intrinsic.Control.EndIf
	F.ODBC.con!rstLot.Close
	Gui.F_Material.lvwParts.SetListItemSubItemText(V.Local.sKey,4,V.Local.fQty)
	F.ODBC.con!rstInv.MoveNext
F.Intrinsic.Control.Loop
F.ODBC.con!rstInv.Close


Program.Sub.Part_Fill.End

