Program.Sub.Issue_Drop.Start
V.Local.sFile.Declare(String)
V.Local.iHdl.Declare(Long)
V.Local.sData.Declare(String)
V.Local.sSQL.Declare(String)
V.Local.sPath.Declare(String)
V.Local.sDate.Declare(String)
V.Local.iRet.Declare(Long)
V.Local.sJob.Declare(String)
V.Local.sSuf.Declare(String)
V.Local.sSeq.Declare(String)
V.Local.sLoc.Declare(String)
V.Local.sLot.Declare(String)
V.Local.sBin.Declare(String)
V.Local.sHeat.Declare(String)
V.Local.sSerial.Declare(String)
V.Local.sPart.Declare(String)
V.Local.sRPart.Declare(String)
V.Local.sDesc.Declare(String)
V.Local.sUM.Declare(String)
V.Local.sProd.Declare(String)

'F.ODBC.Connection!con.OpenRecordsetRO("rstFile",V.Local.sSQL)
'Job and Suffix from Screen, Sequence is SEQ field in V_Job_Operations, Part from Screen, Location, Lot, Bin, Heat, Serial_Number from V_Item_Master, Qty from Screen
'Last 4 Fields Company Code, Workcenter (not sure where to get this), Return leave blank, User is the logged in user for GS
'Drop fields Part is generated based on length and the base part, Loc same as issue,Bin from screen, heat serial come from Issue Material, qty from screen, drop desc = part desc, Drop-UM = issue part um, drop-prod-line=issue part prod line,
'Create file name
F.Intrinsic.String.DateString(V.Ambient.Date,V.Local.sDate)
F.Intrinsic.String.Concat("DIS",V.Local.sDate,".txt",V.Local.sFile)
F.Intrinsic.String.Concat(V.Caller.FilesDir,"\",V.Local.sFile,V.Local.sPath)
'F.Intrinsic.UI.Msgbox(V.Local.sFile)
'get handle for file
F.Intrinsic.File.GetHandle(V.Local.iHdl)
'open file to write data
F.Intrinsic.File.OpenForWrite(V.Local.sPath,V.Local.iHdl)
V.Local.sJob.Set(V.Screen.F_Material!txtWO.Text)
V.Local.sSuf.Set(V.Screen.F_Material!txtWOS.Text)

'code to parse if WO-suffix in one field
'F.Intrinsic.String.Instr(V.Screen.F_Material!txtWO.Text,"-",V.Local.iRet)
'F.Intrinsic.Control.If(V.Local.iRet,>,0)
'	F.Intrinsic.Math.Sub(V.Local.iRet,1,V.Local.iRet)
'	F.Intrinsic.String.Left(V.Screen.F_Material!txtWO.Text,V.Local.iRet,V.Local.sJob)
'	F.Intrinsic.Math.Add(V.Local.iRet,2,V.Local.iRet)
'	F.Intrinsic.String.Mid(V.Screen.F_Material!txtWO.Text,V.Local.iRet,V.Local.sSuf)
'F.Intrinsic.Control.Else
'	V.Local.sJob.Set(V.Screen.F_Material!txtWO.Text)
'	V.Local.sSuf.Set("")
'F.Intrinsic.Control.EndIf
'V.Local.sParam.Set(V.Local.sJob)

'retrieve sequence for the material line of the Job
F.Intrinsic.String.Concat("Select Seq from V_Job_Operations where Job='",V.Local.sJob,"' and Suffix='",V.Local.sSuf,"' and LMO='M' order by Job,Suffix",V.Local.sSQL)
F.ODBC.Connection!con.OpenRecordsetRO("rstOp",V.Local.sSQL)
F.Intrinsic.Control.If(V.ODBC.con!rstOp.EOF,"=",False)
	V.Local.sSeq.Set(V.ODBC.con!rstOp.FieldVal!Seq)
F.Intrinsic.Control.Else
	V.Local.sSeq.Set("00100")
F.Intrinsic.Control.EndIf
F.ODBC.con!rstOp.Close

'start creating data for the file
V.Local.sPart.Set(V.Screen.F_Material!txtPPart.Text)
F.Intrinsic.UI.Msgbox(V.Local.sPart)
F.Intrinsic.String.Concat("Select Part,Product_Line,Description,UM_Inventory from V_Inventory_Mstr where Part='",V.Local.sPart,"'",V.Local.sSQL)
F.ODBC.Connection!con.OpenRecordsetRO("rstDesc",V.Local.sSQL)
F.Intrinsic.Control.If(V.ODBC.con!rstDesc.EOF,"=",False)
	V.Local.sDesc.Set(V.ODBC.con!rstDesc.FieldVal!Description)
	V.Local.sUM.Set(V.ODBC.con!rstDesc.FieldVal!UM_Inventory)
	V.Local.sProd.Set(V.ODBC.con!rstDesc.FieldVal!Product_Line)
F.Intrinsic.Control.Else
	V.Local.sDesc.Set("")
	V.Local.sUM.Set("")
	V.Local.sProd.Set("")
F.Intrinsic.Control.EndIf
F.ODBC.con!rstDesc.Close

F.Intrinsic.String.Concat(V.Local.sJob,"*!*",V.Local.sSuf,"*!*",V.Local.sSeq,"*!*",V.Local.sPart,"*!*",V.Local.sData)

'retrieve Location,Lot, Heat,Serial
F.Intrinsic.String.Concat("Select Location,Lot,Bin,Heat,Serial_Number from V_Item_Master where Part='",V.Local.sPart,"'",V.Local.sSQL)
F.ODBC.Connection!con.OpenRecordsetRO("rstLoc",V.Local.sSQL)
F.Intrinsic.Control.If(V.ODBC.con!rstLoc.EOF,"=",True)
	V.Local.sLoc.Set("")
	V.Local.sLot.Set("")
	V.Local.sBin.Set("")
	V.Local.sHeat.Set("")
	V.Local.sSerial.Set("")
F.Intrinsic.Control.Else
	'the following code makes sure that there is data in each field if not it passes nothing to the parameter
	F.Intrinsic.Control.If(V.ODBC.con!rstLoc.FieldValIsNull!Location,=,True)
		V.Local.sLoc.Set("")
	F.Intrinsic.Control.Else
		V.Local.sLoc.Set(V.ODBC.con!rstLoc.FieldVal!Location)
	F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.If(V.ODBC.con!rstLoc.FieldValIsNull!Lot,=,True)
		V.Local.sLot.Set("")
	F.Intrinsic.Control.Else
		V.Local.sLot.Set(V.ODBC.con!rstLoc.FieldVal!Lot)
	F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.If(V.ODBC.con!rstLoc.FieldValIsNull!Bin,=,True)
		V.Local.sBin.Set("")
	F.Intrinsic.Control.Else
		V.Local.sBin.Set(V.ODBC.con!rstLoc.FieldVal!Bin)
	F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.If(V.ODBC.con!rstLoc.FieldValIsNull!Heat,=,True)
		V.Local.sHeat.Set("")
	F.Intrinsic.Control.Else
		V.Local.sHeat.Set(V.ODBC.con!rstLoc.FieldVal!Heat)
	F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.If(V.ODBC.con!rstLoc.FieldValIsNull!Serial_Number,=,True)
		V.Local.sSerial.Set("")
	F.Intrinsic.Control.Else
		V.Local.sSerial.Set(V.ODBC.con!rstLoc.FieldVal!Serial_Number)
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf
F.ODBC.con!rstLoc.Close

'adding Part,Location,Lot,Bin,Serial,Qty to data for file
F.Intrinsic.String.Concat(V.Local.sData,V.Local.sLoc,"*!*",V.Local.sLot,"*!*",V.Local.sBin,"*!*",V.Local.sHeat,"*!*",V.Local.sSerial,"*!*",V.Screen.F_Material!txtPull.Text,"*!*",V.Local.sData)

'creating drop information
'first drop
F.Intrinsic.Control.If(V.Screen.F_Material!txtRQty.Text,<>,"")
	F.Intrinsic.String.Format(V.Screen.F_Material!txtRLen.Text,"000.000",V.Local.sRPart)
	F.Intrinsic.String.Concat(V.Screen.F_Material!txtBaseP.Text,"-",V.Local.sRPart,V.Local.sRPart)
	F.Intrinsic.String.Concat(V.Local.sData,V.Local.sRPart,"*!*",V.Local.sLoc,"*!*",V.Local.sLot,"*!*",V.Screen.F_Material!txtRBin.Text,"*!*",V.Local.sHeat,"*!*",V.Local.sSerial,"*!*",V.Screen.F_Material!txtRQty.Text,"*!*",V.Local.sDesc,"*!*",V.Local.sUM,"*!*",V.Local.sProd,"*!*",V.Local.sData)
F.Intrinsic.Control.Else
	F.Intrinsic.String.Concat(V.Local.sData,"*!**!**!**!**!**!**!**!**!**!*",V.Local.sData)
F.Intrinsic.Control.EndIf
'take out when additional drops are available
'F.Intrinsic.Control.If(1,=,2)
'2nd Drop
F.Intrinsic.Control.If(V.Screen.F_Material!txtR2Qty.Text,<>,"")
	F.Intrinsic.String.Format(V.Screen.F_Material!txtR2Len.Text,"000.000",V.Local.sRPart)
	F.Intrinsic.String.Concat(V.Screen.F_Material!txtBaseP.Text,"-",V.Local.sRPart,V.Local.sRPart)
	F.Intrinsic.String.Concat(V.Local.sData,V.Local.sRPart,"*!*",V.Local.sLoc,"*!*",V.Local.sLot,"*!*",V.Screen.F_Material!txtR2Bin.Text,"*!*",V.Local.sHeat,"*!*",V.Local.sSerial,"*!*",V.Screen.F_Material!txtRQty.Text,"*!*",V.Local.sDesc,"*!*",V.Local.sUM,"*!*",V.Local.sProd,"*!*",V.Local.sData)
F.Intrinsic.Control.Else
	F.Intrinsic.String.Concat(V.Local.sData,"*!**!**!**!**!**!**!**!**!**!*",V.Local.sData)
F.Intrinsic.Control.EndIf
'3rd Drop
F.Intrinsic.Control.If(V.Screen.F_Material!txtR3Qty.Text,<>,"")
	F.Intrinsic.String.Format(V.Screen.F_Material!txtR3Len.Text,"000.000",V.Local.sRPart)
	F.Intrinsic.String.Concat(V.Screen.F_Material!txtBaseP.Text,"-",V.Local.sRPart,V.Local.sRPart)
	F.Intrinsic.String.Concat(V.Local.sData,V.Local.sRPart,"*!*",V.Local.sLoc,"*!*",V.Local.sLot,"*!*",V.Screen.F_Material!txtR3Bin.Text,"*!*",V.Local.sHeat,"*!*",V.Local.sSerial,"*!*",V.Screen.F_Material!txtRQty.Text,"*!*",V.Local.sDesc,"*!*",V.Local.sUM,"*!*",V.Local.sProd,"*!*",V.Local.sData)
F.Intrinsic.Control.Else
	F.Intrinsic.String.Concat(V.Local.sData,"*!**!**!**!**!**!**!**!**!**!*",V.Local.sData)
F.Intrinsic.Control.EndIf
'4th Drop
F.Intrinsic.Control.If(V.Screen.F_Material!txtR4Qty.Text,<>,"")
	F.Intrinsic.String.Format(V.Screen.F_Material!txtR4Len.Text,"000.000",V.Local.sRPart)
	F.Intrinsic.String.Concat(V.Screen.F_Material!txtBaseP.Text,"-",V.Local.sRPart,V.Local.sRPart)
	F.Intrinsic.String.Concat(V.Local.sData,V.Local.sRPart,"*!*",V.Local.sLoc,"*!*",V.Local.sLot,"*!*",V.Screen.F_Material!txtR4Bin.Text,"*!*",V.Local.sHeat,"*!*",V.Local.sSerial,"*!*",V.Screen.F_Material!txtRQty.Text,"*!*",V.Local.sDesc,"*!*",V.Local.sUM,"*!*",V.Local.sProd,"*!*",V.Local.sData)
F.Intrinsic.Control.Else
	F.Intrinsic.String.Concat(V.Local.sData,"*!**!**!**!**!**!**!**!**!**!*",V.Local.sData)
F.Intrinsic.Control.EndIf
'F.Intrinsic.Control.EndIf

'create the User specific information
F.Intrinsic.String.Concat(V.Local.sData,V.Caller.CompanyCode,"*!**!*",V.Caller.User,V.Local.sData)

'test file data
'V.Local.sData.Set("000002*!*   *!*001000*!*MB1022-013.000*!*  *!*LOT5689*!*BN1360*!*HEAT45645*!*SERIAL132323*!*2*!**!**!**!**!**!**!**!**!**!**!*ENM*!**!**!*SUPERVSR*!*")

'write data to file
F.Intrinsic.File.WriteLine(V.Local.iHdl,V.Local.sData)

'close file
F.Intrinsic.File.CloseFile(V.Local.iHdl)

'GSS Call within GAB
F.Intrinsic.Task.LaunchGSSAsync("JB0075CL","-C",V.Local.sFile,V.Local.iHdl)

'Call Wip to FG
F.Intrinsic.Control.CallSub(Wip_fg_file)




Program.Sub.Issue_Drop.End

