Program.Sub.LVW_Fill.Start
V.Local.sSQL.Declare(String)
V.Local.sDate.Declare(String)
V.Local.sRet.Declare(String)
V.Local.sTitle.Declare(String)
V.Local.iCt.Declare(Long)
V.Local.sJob.Declare(String)
V.Local.sOrder.Declare(String)

Gui.F_Distro.lvwDistro.MultiSelect(true)
Gui.F_Distro.lvwDistro.ClearItems
Gui.F_Distro.cmdFlex.Enabled(True)
Gui.F_Distro.cmdWO.Enabled(true)
Gui.F_Distro.cmdWOPick.Enabled(true)

'set the rst order based on the option selected
F.Intrinsic.Control.If(V.Screen.F_Distro!optPri.Value,=,True)
	F.ODBC.Connection!con.OpenRecordsetRO("rstWO","Select * from V_Job_Header where Process_Grp='D'  and Date_Closed='1900-01-01' order by Priority desc,Part desc")
F.Intrinsic.Control.ElseIf(V.Screen.F_Distro!optSO.Value,=,True)
	F.ODBC.Connection!con.OpenRecordsetRO("rstWO","Select Order_No,Record_No,Customer from V_Order_Header where Process_Grp ='D' order by Order_No")
	Gui.F_Distro.cmdFlex.Enabled(False)
	Gui.F_Distro.cmdWO.Enabled(False)
	Gui.F_Distro.cmdWOPick.Enabled(False)
F.Intrinsic.Control.ElseIf(V.Screen.F_Distro!optPart.Value,=,True)
	F.ODBC.Connection!con.OpenRecordsetRO("rstWO","Select * from V_Job_Header where Process_Grp='D' and Date_Closed='1900-01-01' order by Part desc,Job,Suffix,Priority desc")
F.Intrinsic.Control.ElseIf(V.Screen.F_Distro!optCust.Value,=,True)
	F.ODBC.Connection!con.OpenRecordsetRO("rstWO","Select * from V_Job_Header where Process_Grp='D' and Date_Closed='1900-01-01' order by Customer,Part desc,Job,Suffix,Priority desc")
F.Intrinsic.Control.ElseIf(V.Screen.F_Distro!optWO.Value,=,True)
	F.ODBC.Connection!con.OpenRecordsetRO("rstWO","Select * from V_Job_Header where Process_Grp='D' and Date_Closed='1900-01-01' order by Job,Suffix,Priority desc")
F.Intrinsic.Control.ElseIf(V.Screen.F_Distro!optDate.Value,=,True)
	F.ODBC.Connection!con.OpenRecordsetRO("rstWO","Select * from V_Job_Header where Process_Grp='D' and Date_Closed='1900-01-01' order by Date_Due,Part desc,Job,Suffix,Priority desc")
'F.Intrinsic.Control.ElseIf(V.Screen.F_Distro!optSched.Value,=,True)
'	F.ODBC.Connection!con.OpenRecordsetRO("rstWO","Select * from V_Job_Header where Process_Grp='D' order by Project,Job,Suffix,Priority desc")
F.Intrinsic.Control.Else
	'F.Intrinsic.UI.Msgbox("Select * from Order_Header where Record_Type='A' and Process_Grp ='D' order by Order_No")
	F.ODBC.Connection!con.OpenRecordsetRO("rstWO","Select * from V_Job_Header where Process_Grp='D' and Date_Closed='1900-01-01' order by Part desc,Job,Suffix,Priority desc")
F.Intrinsic.Control.EndIf

'cycle through the recordset to get data
F.Intrinsic.Control.DoUntil(V.ODBC.con!rstWO.EOF,=,True)
	F.Intrinsic.Control.If(V.Screen.F_Distro!optSO.Value,=,False)
		F.Intrinsic.Control.If(V.ODBC.con!rstWO.FieldValTrim!Suffix,=,"")
			V.Local.sJob.Set(V.ODBC.con!rstWO.FieldVal!Job)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.ODBC.con!rstWO.FieldVal!Job,"-",V.ODBC.con!rstWO.FieldVal!Suffix,V.Local.sJob)
		F.Intrinsic.Control.EndIf
		'F.Intrinsic.UI.Msgbox(V.ODBC.con!rstWO.FieldVal!date_Due)
		F.Intrinsic.Control.If(V.ODBC.con!rstWO.FieldVal!Date_Due,=,"1900-01-01")
			Gui.F_Distro.lvwDistro.AddListItem(V.Local.sJob,"")
		F.Intrinsic.Control.Else
			Gui.F_Distro.lvwDistro.AddListItem(V.Local.sJob,V.ODBC.con!rstWO.FieldVal!Date_Due)
		F.Intrinsic.Control.EndIf
		'need to get Customer Name
		F.Intrinsic.String.Concat("Select Name_Customer from V_Customer_Master where Customer='",V.ODBC.con!rstWO.FieldVal!Customer,"'",V.Local.sSQL)
		F.ODBC.Connection!con.OpenRecordsetRO("rstCust",V.Local.sSQL)
		Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sJob,1,V.ODBC.con!rstCust.FieldVal!Name_Customer)
		F.ODBC.con!rstCust.Close
		Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sJob,2,V.ODBC.con!rstWO.FieldVal!Part)
		'calculate onhand qty
		F.Intrinsic.String.Concat("Select QTY_ONHAND from V_Inventory_Mstr where Part='",V.ODBC.con!rstWO.FieldVal!Part,"'",V.Local.sSQL)
		F.ODBC.Connection!con.OpenRecordsetRO("rstQty",V.Local.sSQL)
		F.Intrinsic.Control.If(V.ODBC.con!rstQty.EOF,=,False)
			Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sJob,3,V.ODBC.con!rstQty.FieldVal!Qty_Onhand)
		F.Intrinsic.Control.Else
			Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sJob,3,0)
		F.Intrinsic.Control.EndIf
		F.ODBC.con!rstQty.Close
		Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sJob,4,V.ODBC.con!rstWO.FieldVal!Qty_Order)
		F.Intrinsic.Control.If(V.ODBC.con!rstWO.FieldValIsNull!Suffix,=,False)
			F.Intrinsic.String.Concat("Select Order_No,Order_Line from V_Order_To_WO where Job='",V.ODBC.con!rstWO.FieldVal!Job,"' and Suffix='",V.ODBC.con!rstWO.FieldVal!Suffix,"'",V.Local.sSQL)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat("Select Order_No,Order_Line from V_Order_To_WO where Job='",V.ODBC.con!rstWO.FieldVal!Job,"'",V.Local.sSQL)
		F.Intrinsic.Control.EndIf
		F.ODBC.Connection!con.OpenRecordsetRO("rstLU",V.Local.sSQL)
		F.Intrinsic.Control.If(V.ODBC.con!rstLU.EOF,=,False)
			F.Intrinsic.String.Concat(V.ODBC.con!rstLU.FieldVal!Order_No,"-",V.ODBC.con!rstLU.FieldVal!Order_Line,V.Local.sOrder)
		F.Intrinsic.Control.Else
			V.Local.sOrder.Set("")
		F.Intrinsic.Control.EndIf
		F.ODBC.con!rstLU.Close
		Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sJob,5,V.local.sOrder)
		Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sJob,6,V.Local.sJob)
		Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sJob,7,V.ODBC.con!rstWO.FieldVal!Priority)
		'need to concat status field for the next two.
		Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sJob,8,V.ODBC.con!rstWO.FieldVal!Qty_Completed)
		Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sJob,9,V.ODBC.con!rstWO.FieldVal!Qty_Completed)
	F.Intrinsic.Control.Else
		'F.Intrinsic.UI.Msgbox("Else")
		F.Intrinsic.String.Concat("Select * from V_Order_Lines where Order_No='",V.ODBC.con!rstWO.FieldVal!Order_No,"' order by Order_No,Record_No,Date_Item_Prom,Part",V.Local.sSQL)
		F.ODBC.Connection!con.OpenRecordsetRO("rstL",V.Local.sSQL)
		F.Intrinsic.Control.DoUntil(V.ODBC.con!rstL.EOF,=,True)
			'F.Intrinsic.UI.Msgbox("Records")
			'F.Intrinsic.Control.If(V.ODBC.con!rstWO.FieldValTrim!Suffix,=,"")
			'	V.Local.sJob.Set(V.ODBC.con!rstWO.FieldVal!Job)
			'F.Intrinsic.Control.Else
				F.Intrinsic.String.Concat(V.ODBC.con!rstL.FieldVal!Order_No,"-",V.ODBC.con!rstL.FieldVal!Record_No,V.Local.sOrder)
			'F.Intrinsic.Control.EndIf
			Gui.F_Distro.lvwDistro.AddListItem(V.Local.sOrder,V.ODBC.con!rstL.FieldVal!Date_Item_Prom)
			'need to get Customer Name
			F.Intrinsic.String.Concat("Select Name_Customer from V_Customer_Master where Customer='",V.ODBC.con!rstL.FieldVal!Customer,"'",V.Local.sSQL)
			F.ODBC.Connection!con.OpenRecordsetRO("rstCust",V.Local.sSQL)
			Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sOrder,1,V.ODBC.con!rstCust.FieldVal!Name_Customer)
			F.ODBC.con!rstCust.Close
			Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sOrder,2,V.ODBC.con!rstL.FieldVal!Part)
			'calculate onhand qty
			F.Intrinsic.String.Concat("Select QTY_ONHAND from V_Inventory_Mstr where Part='",V.ODBC.con!rstL.FieldVal!Part,"'",V.Local.sSQL)
			F.ODBC.Connection!con.OpenRecordsetRO("rstQty",V.Local.sSQL)
			F.Intrinsic.Control.If(V.ODBC.con!rstQty.EOF,=,False)
				Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sOrder,3,V.ODBC.con!rstQty.FieldVal!Qty_Onhand)
			F.Intrinsic.Control.Else
				Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sOrder,3,0)
			F.Intrinsic.Control.EndIf
			F.ODBC.con!rstQty.Close
			Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sOrder,4,V.ODBC.con!rstL.FieldVal!Qty_Ordered)
			Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sOrder,5,V.Local.sOrder)
			F.Intrinsic.String.Concat("Select Job,Suffix from V_Order_To_WO where Order_No='",V.ODBC.con!rstL.FieldVal!Order_No,"' and Order_Line='",V.ODBC.con!rstL.FieldVal!Record_No,"'",V.Local.sSQL)
			F.ODBC.Connection!con.OpenRecordsetRO("rstLU",V.Local.sSQL)
			F.Intrinsic.Control.If(V.ODBC.con!rstLU.EOF,=,False)
				V.Local.sJob.Set(V.odbc.con!rstLU.FieldVal!Job)
				F.Intrinsic.Control.If(V.ODBC.con!rstLU.FieldValIsNull!Suffix,=,False)
					F.Intrinsic.String.Concat(V.Local.sJob,"-",V.ODBC.con!rstLU.FieldVal!Suffix,V.Local.sJob)
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.Else
					V.Local.sJob.Set("")
			F.Intrinsic.Control.EndIf
			F.ODBC.con!rstLU.Close
			Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sOrder,6,V.Local.sJob)
			'Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sJob,7,V.ODBC.con!rstWO.FieldVal!Priority)
			'need to concat status field for the next two.
			Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sOrder,8,V.ODBC.con!rstL.FieldVal!Qty_Shipped)
			Gui.F_Distro.lvwDistro.SetListItemSubItemText(V.Local.sOrder,9,V.ODBC.con!rstL.FieldVal!Qty_Shipped)
			F.ODBC.con!rstL.MoveNext
	F.Intrinsic.Control.Loop
	F.Intrinsic.Control.EndIf
	F.ODBC.con!rstWO.MoveNext
F.Intrinsic.Control.Loop
Gui.F_Distro..Show
F.ODBC.con!rstWO.Close

Program.Sub.LVW_Fill.End

Program.Sub.Luanch_Issue.Start
V.Local.iRet.Declare(Long)
V.Local.sJob.Declare(String)
V.Local.sParam.Declare(String)
V.Local.sPart.Declare(String)
V.Local.iPos.Declare(Long)
V.Local.sOrder.Declare(String)
V.Local.sSuf.Declare(String)
V.Local.sWO.Declare(String)

'gets the Part, Order, and WO
V.Local.sPart.Set(V.Screen.F_Distro!lvwDistro.SelectedItemSubItem(2))
V.Local.sOrder.Set(V.Screen.F_Distro!lvwDistro.SelectedItemSubItem(5))
V.Local.sWO.Set(V.Screen.F_Distro!lvwDistro.SelectedItemSubItem(6))

'parses the Job from the Suffix
'F.Intrinsic.UI.Msgbox(V.Local.sWO)
F.Intrinsic.String.Instr(V.local.sWO,"-",V.Local.iRet)
F.Intrinsic.Control.If(V.Local.iRet,>,0)
	F.Intrinsic.Math.Sub(V.Local.iRet,1,V.Local.iRet)
	F.Intrinsic.String.Left(V.Local.sWO,V.Local.iRet,V.Local.sJob)
	F.Intrinsic.Math.Add(V.Local.iRet,2,V.Local.iRet)
	F.Intrinsic.String.Left(V.Local.sWO,V.Local.iRet,V.Local.sSuf)
F.Intrinsic.Control.else
	V.Local.sJob.Set(V.Local.sWO)
	V.Local.sSuf.Set("")
F.Intrinsic.Control.endif

'sets the Param to the Job - not really sure why
V.Local.sParam.Set(V.Local.sJob)
'F.Intrinsic.Variable.AddPV("sPart",V.Local.sPart)

'gets the Base Part
F.Intrinsic.String.Instr(V.Local.sPart,"-",V.Local.iPos)
F.Intrinsic.Control.If(V.Local.iPos,>,0)
	F.Intrinsic.Math.Sub(V.Local.iPos,1,V.Local.iPos)
	F.Intrinsic.String.Left(V.Local.sPart,V.Local.iPos,V.Local.sPart)
F.Intrinsic.Control.EndIf
'Gui.F_Material.txtWO.Text(V.Local.sJob)

'calls the sub "Part_Fill" and passes the job, suffix, part, and order
F.Intrinsic.Control.CallSub(Part_fill,"sPart",V.Local.sPart,"sJob",V.Local.sJob,"sSuf",V.local.sSuf,"sOrder",V.Local.sOrder)
'shows the F_Material screen
Gui.F_Material..Show

Program.Sub.Luanch_Issue.End

