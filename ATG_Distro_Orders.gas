F.Intrinsic.String.Concat(V.Local.sJob,"*!*",V.Local.sSuf,"*!*",V.Local.sRPart,"*!*",V.Local.sData)

'retrieve Location,Lot, Heat,Serial
F.Intrinsic.String.Concat("Select Location,Lot,Heat,Serial_Number from V_Item_Master where Part='",V.Local.sPart,"'",V.Local.sSQL)
F.ODBC.Connection!con.OpenRecordsetRO("rstLoc",V.Local.sSQL)
F.Intrinsic.Control.If(V.ODBC.con!rstLoc.EOF,"=",True)
	V.Local.sLot.Set("")
	V.Local.sHeat.Set("")
	V.Local.sSerial.Set("")
	V.Local.sLoc.Set("")
F.Intrinsic.Control.Else
	F.Intrinsic.Control.If(V.ODBC.con!rstLoc.FieldValIsNull!Location,=,True)
		V.Local.sLoc.Set("")
	F.Intrinsic.Control.Else
		V.Local.sLoc.Set(V.ODBC.con!rstLoc.FieldVal!Location)
	F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.If(V.ODBC.con!rstLoc.FieldValIsNull!Lot,=,True)
		V.Local.sLot.Set("")
	F.Intrinsic.Control.Else
		V.Local.sLot.Set(V.ODBC.con!rstLoc.FieldVal!Lot)
	F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.If(V.ODBC.con!rstLoc.FieldValIsNull!Heat,=,True)
		V.Local.sHeat.Set("")
	F.Intrinsic.Control.Else
		V.Local.sHeat.Set(V.ODBC.con!rstLoc.FieldVal!Heat)
	F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.If(V.ODBC.con!rstLoc.FieldValIsNull!Serial_Number,=,True)
		V.Local.sSerial.Set("")
	F.Intrinsic.Control.Else
		V.Local.sSerial.Set(V.ODBC.con!rstLoc.FieldVal!Serial_Number)
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf
F.ODBC.con!rstLoc.Close

'add company, WC, and user
F.Intrinsic.String.Concat(V.Local.sData,V.Local.sLoc,"*!*",V.Screen.F_Material!txtSQty.Text,"*!*",V.Caller.CompanyCode,"*!**!*",V.Caller.User,"*!*",V.Local.sData)


F.Intrinsic.String.Concat(V.Local.sData,V.Local.sLot,"*!*",V.Screen.F_Material!txtSBin.Text,"*!*",V.Local.sHeat,"*!*",V.Local.sSerial,V.Local.sData)

'V.Local.sData.Set("000002*!*   *!*MB1022-013.000*!* *!*2*!*ENM*!* *!*SUPERVSR*!*LOT5689*!*SSS*!**!**!*")
'F.Intrinsic.String.Concat(V.Local.sData,"Part","*!*","Loc","*!*,"FGQTY","*!*","Company","*!*",V.local.sData)
F.Intrinsic.File.WriteLine(V.Local.iHdl,V.Local.sData)
F.Intrinsic.File.CloseFile(V.Local.iHdl)
F.Intrinsic.Task.LaunchGSSAsync("JB0052CL","-C",V.Local.sFile,V.Local.iHdl)

