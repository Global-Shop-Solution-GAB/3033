Program.Sub.Print_WO.Start
V.Local.iRet.Declare(Long)
V.Local.sPart.Declare(String)
V.Local.sJob.Declare(String)
V.Local.sSuf.Declare(String)
V.Local.sParam.Declare(String)
V.Local.sSQL.Declare(String)

'F.Intrinsic.Debug.Stop
V.Local.sPart.Set(V.Screen.F_Distro!lvwDistro.SelectedItemSubItem(2))
V.Local.sJob.Set(V.Screen.F_Distro!lvwDistro.SelectedItemSubItem(6))
F.Intrinsic.String.Instr(V.local.sJob,"-",V.Local.iRet)
F.Intrinsic.Control.If(V.Local.iRet,=,0)
	V.Local.sSuf.Set("*!*ZDR*!* ")
F.Intrinsic.Control.Else
	F.Intrinsic.String.Left(V.Local.sJob,V.Local.iRet,V.Local.sJob)
	F.Intrinsic.Math.Add(V.Local.iRet,1,V.Local.iRet)
	F.Intrinsic.String.Mid(V.Local.sJob,V.Local.iRet,V.Local.sSuf)
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.If(V.Local.sSuf,=,"*!*ZDR*!* ")
	F.Intrinsic.String.Concat("Select * from ATG_MB_JobStatus where Job='",V.Local.sJob,"'",V.Local.sSQL)
	F.ODBC.Connection!con.OpenRecordsetRW("rstSt",V.Local.sSQL)
	F.Intrinsic.Control.If(V.ODBC.con!rstSt.EOF,=,True)
		F.ODBC.con!rstSt.AddNew
		F.ODBC.con!rstSt.Set!Job(V.Local.sJob)
		F.ODBC.con!rstSt.Set!Suffix("")
		F.ODBC.con!rstSt.Set!Printed(1)
		F.ODBC.con!rstSt.Set!Print_Date(V.Ambient.Now)
		'F.ODBC.con!rstSt.Set!WIP_Qty(V.Screen.F_Distro!lvwDistro.SelectedItemSubItem(5))
		F.ODBC.con!rstSt.Update
	F.Intrinsic.Control.Else
		F.ODBC.con!rstSt.Set!Printed(1)
		F.ODBC.con!rstSt.Set!Print_Date(V.Ambient.Now)
		'F.ODBC.con!rstSt.Set!WIP_Qty(V.Screen.F_Distro!lvwDistro.SelectedItemSubItem(5))
		F.ODBC.con!rstSt.Update
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.Else
	F.Intrinsic.String.Concat("Select * from ATG_MB_JobStatus where Job='",V.Local.sJob,"' and Suffix='",V.Local.sSuf,"'",V.Local.sSQL)
	F.ODBC.Connection!con.OpenRecordsetRW("rstSt",V.Local.sSQL)
	F.Intrinsic.Control.If(V.ODBC.con!rstSt.EOF,=,True)
		F.ODBC.con!rstSt.AddNew
		F.ODBC.con!rstSt.Set!Job(V.Local.sJob)
		F.ODBC.con!rstSt.Set!Suffix(V.Local.sSuf)
		F.ODBC.con!rstSt.Set!Printed(1)
		F.ODBC.con!rstSt.Set!Print_Date(V.Ambient.Now)
		'F.ODBC.con!rstSt.Set!WIP_Qty(V.Screen.F_Distro!lvwDistro.SelectedItemSubItem(5))
		F.ODBC.con!rstSt.Update
	F.Intrinsic.Control.Else
		F.ODBC.con!rstSt.Set!Printed(1)
		F.ODBC.con!rstSt.Set!Print_Date(V.Ambient.Now)
		'F.ODBC.con!rstSt.Set!WIP_Qty(V.Screen.F_Distro!lvwDistro.SelectedItemSubItem(5))
		F.ODBC.con!rstSt.Update
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf

F.ODBC.con!rstSt.Close
F.Intrinsic.String.Concat(V.Local.sJob,"!*!",V.Local.sSuf,"!*!001811!*!",V.Local.sParam)
'F.Intrinsic.UI.Msgbox(V.Local.sParam)
F.Global.General.CallWrapperASync(900100,V.Local.sParam)

Program.Sub.Print_WO.End

