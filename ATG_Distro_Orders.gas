Program.Sub.Bin_Browser.Start
Variable.local.sRet.Declare(String)
Variable.local.sTemp.Declare(String)
Variable.local.sTitles.Declare(String)
Variable.local.iWidths.Declare(Long)
V.Local.iCt.Declare(Long)

'sees which button called the routine
F.Intrinsic.Control.If(V.Screen.F_Material!cmdR1Bin.Value,=,True)
	V.Local.iCt.Set(1)
F.Intrinsic.Control.Elseif(V.Screen.F_Material!cmdR2Bin.Value,=,True)
	V.Local.iCt.Set(2)
F.Intrinsic.Control.Elseif(V.Screen.F_Material!cmdR3Bin.Value,=,True)
	V.Local.iCt.Set(3)
F.Intrinsic.Control.Elseif(V.Screen.F_Material!cmdR4Bin.Value,=,True)
	V.Local.iCt.Set(4)
F.Intrinsic.Control.Endif

'creates the mini browser
Function.intrinsic.string.split("Bin*!*Description","*!*",Variable.local.sTitles)
Function.intrinsic.string.split("800*!*3000","*!*",Variable.local.iWidths)
Function.intrinsic.ui.minibrowser("Select a Bin",con,"Select Bin,Bin_Description From V_BIN_Master order by BIN",Variable.local.sTitles,Variable.local.iWidths,Variable.local.sRet)
'if no Bin is selected a message box will appear
Function.intrinsic.control.if(Variable.local.sRet,"=","***CANCEL***")
	Function.intrinsic.ui.msgbox("No bin was selected!")
'when there is a selected bin it retrieves the data and puts it in the appropriate field.
Function.intrinsic.control.else
	Function.intrinsic.string.split(Variable.local.sRet,"*!*",Variable.local.sRet)
	'Function.intrinsic.string.concat("You selected ",Variable.local.sRet(0),"-",Variable.local.sRet(1),Variable.local.sTemp)
	'Function.intrinsic.ui.msgbox(Variable.local.sTemp)
	F.Intrinsic.Control.SelectCase(V.Local.iCt)
		F.Intrinsic.Control.Case(1)
			Gui.F_Material.txtRBin.Text(V.Local.sRet(0))
		F.Intrinsic.Control.Case(2)
			Gui.F_Material.txtR2Bin.Text(V.Local.sRet(0))
		F.Intrinsic.Control.Case(3)
			Gui.F_Material.txtR3Bin.Text(V.Local.sRet(0))
		F.Intrinsic.Control.Case(4)
			Gui.F_Material.txtR4Bin.Text(V.Local.sRet(0))
		F.Intrinsic.Control.CaseElse
			Gui.F_Material.txtRBin.Text(V.Local.sRet(0))
	F.Intrinsic.Control.EndSelect
Function.intrinsic.control.endif

Program.Sub.Bin_Browser.End

Program.Sub.Print_Flex.Start
V.Local.sRet.Declare(String)
V.Local.iRet.Declare(Long)
V.Local.sJob.Declare(String)
V.Local.sSuf.Declare(String)
V.Local.sParam.Declare(String)
V.Local.sWO.Declare(String)
V.Local.sSQL.Declare(String)

'get Job and Suffix split
V.Local.sWO.Set(V.Screen.F_Distro!lvwDistro.SelectedItemSubItem(6))
F.Intrinsic.String.Instr(V.local.sWO,"-",V.Local.iRet)
F.Intrinsic.Control.If(V.Local.iRet,=,0)
	V.Local.sJob.Set(V.Local.sWO)
	V.Local.sSuf.Set("*!*ZDR*!*")
F.Intrinsic.Control.Else
	F.Intrinsic.Math.Sub(V.Local.iRet,1,V.Local.iRet)
	F.Intrinsic.String.Left(V.Local.sWO,V.Local.iRet,V.Local.sJob)
	F.Intrinsic.Math.Add(V.Local.iRet,2,V.Local.iRet)
	F.Intrinsic.String.Mid(V.Local.sWO,V.Local.iRet,V.Local.sSuf)
F.Intrinsic.Control.EndIf

'get data from Flex Schedule Table
F.Intrinsic.String.Concat("Select Schedule,Schedule_Suffix,Schedule_Seq,Date_Sched_From,Date_Sched_To from V_Job_CT_Flx_Schd where Job='",V.Local.sJob,"' and Suffix='",V.Local.sSuf,"' order by Schedule,Schedule_Suffix,Schedule_Seq",V.Local.sSQL)
F.ODBC.Connection!con.OpenRecordsetRO("rstFlex",V.Local.sSQL)
F.Intrinsic.control.If(V.ODBC.con!rstFlex.EOF,=,True)
	V.local.sParam.Set("!*!!*!!*!!*!!*!!*!!*!!*!")
F.Intrinsic.Control.Else
	'starting date
	F.Intrinsic.Control.If(V.ODBC.con!rstFlex.FieldValIsNull!Date_Sched_From,=,True)
		V.Local.sParam.Set("!*!")
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Concat(V.ODBC.con!rstFlex.FieldVal!Date_Sched_From,"!*!",V.Local.sParam)
	F.Intrinsic.Control.EndIf
	'ending date
	F.Intrinsic.Control.If(V.ODBC.con!rstFlex.FieldValIsNull!Date_Sched_To,=,True)
		F.Intrinsic.String.Concat(V.Local.sParam,"!*!",V.Local.sParam)
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Concat(V.Local.sParam,"!*!",V.ODBC.con!rstFlex.FieldVal!Date_Sched_To,"!*!",V.Local.sParam)
	F.Intrinsic.Control.EndIf
	'starting schedule
	F.Intrinsic.Control.If(V.ODBC.con!rstFlex.FieldValIsNull!Schedule,=,True)
		F.Intrinsic.String.Concat(V.Local.sParam,"!*!",V.Local.sParam)
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Concat(V.Local.sParam,"!*!",V.ODBC.con!rstFlex.FieldVal!Schedule,"!*!",V.Local.sParam)
	F.Intrinsic.Control.EndIf
	'starting suffix
	F.Intrinsic.Control.If(V.ODBC.con!rstFlex.FieldValIsNull!Schedule_Suffix,=,True)
		F.Intrinsic.String.Concat(V.Local.sParam,"!*!",V.Local.sParam)
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Concat(V.Local.sParam,"!*!",V.ODBC.con!rstFlex.FieldVal!Schedule_Suffix,"!*!",V.Local.sParam)
	F.Intrinsic.Control.EndIf
	'get ending info
	F.Intrinsic.String.Concat("Select Schedule,Schedule_Suffix,Schedule_Seq,Date_Sched_From,Date_Sched_To from V_Job_CT_Flx_Schd where Job='",V.Local.sJob,"' and Suffix='",V.Local.sSuf,"' order by Schedule desc,Schedule_Suffix desc,Schedule_Seq desc",V.Local.sSQL)
	F.ODBC.Connection!con.OpenRecordsetRO("rstFlex2",V.Local.sSQL)
	F.Intrinsic.control.If(V.ODBC.con!rstFlex.EOF,=,True)
		F.Intrinsic.String.Concat(V.Local.sParam,V.ODBC.con!rstFlex.FieldVal!Schedule,"!*!",V.ODBC.con!rstFlex.FieldVal!Schedule_Suffix,"!*!",V.Local.sParam)
	F.Intrinsic.Control.Else
		'ending schedule
		F.Intrinsic.Control.If(V.ODBC.con!rstFlex2.FieldValIsNull!Schedule,=,True)
			F.Intrinsic.String.Concat(V.Local.sParam,"!*!",V.Local.sParam)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sParam,"!*!",V.ODBC.con!rstFlex2.FieldVal!Schedule,"!*!",V.Local.sParam)
		F.Intrinsic.Control.EndIf
		'ending suffix
		F.Intrinsic.Control.If(V.ODBC.con!rstFlex2.FieldValIsNull!Schedule_Suffix,=,True)
			F.Intrinsic.String.Concat(V.Local.sParam,"!*!",V.Local.sParam)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sParam,"!*!",V.ODBC.con!rstFlex2.FieldVal!Schedule_Suffix,"!*!",V.Local.sParam)
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf
'F.Intrinsic.String.Concat(V.Local.sJob,"!*!",V.Local.sSuf,"!*!",V.Local.sJob,"!*!",V.Local.sSuf,V.Local.sParam)
'add Y for show Material, and Y for Extended Bar Code
F.Intrinsic.String.Concat(V.Local.sParam,"Y!*!Y!*!",V.Local.sParam)

'Call the program to launch the Flex Schedule
f.Global.General.CallWrapperASync(900300,V.Local.sParam)


Program.Sub.Print_Flex.End

