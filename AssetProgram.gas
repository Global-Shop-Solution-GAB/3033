Program.Sub.Asset_Lookup.Start
V.Local.sTitle.Declare(String)
V.Local.iWidth.Declare(String)
V.local.sSQL.Declare(String)
V.local.sRet.Declare(String)
V.Local.sText.Declare(String)
V.Local.Datex.Declare(Date)
V.Local.iTemp.Declare(Long)

F.Intrinsic.String.Split("Asset ID *!*Tag ID*!* Hw_Sw*!*Category*!*SubCategory*!*Make*!*Model","*!*",V.Local.sTitle)
F.Intrinsic.String.Split("1000*!*2000*!*0*!*2000*!*1500*!*1000*!*1500*!*0*!*0*!*0*!*0*!*0*!*0*!*0","*!*",V.Local.iWidth)
'F.Intrinsic.String.Split("AID*!*HW_SW*!*Tag ID*!*Category*!*Sub-Category*!*Make*!*Model","*!*",V.Local.sTitle)
'F.Intrinsic.String.Split("0*!*0*!*2000*!*2000*!*2000*!*2000*!*2000","*!*",V.Local.sWidth)

'V.Local.sSQL.Set("Select A.Asset_ID, A.Tag_ID,C.Hw_Sw, C1.Description, C.Description, M1.Description, M2.Description, V.Company_Name, A.Invoice, A.Serial_Number, A.Asset_Description, A.Cost, A.VID, A.Date_Received FROM AM_Asset as A, AM_Make as M1, AM_Make as M2, AM_Asset_Catergory as C, AM_Asset_Catergory as C1, AM_Vendor_Details as V WHERE A.Asset_Category_ID = C1.Asset_Category_ID AND A.Make = M1.Asset_Make_ID AND C.Asset_Category_ID = M1.subcatID AND M1.Asset_Make_ID = M2.Parent AND A.Model = M2.Asset_Make_ID AND A.VID = V.Vendor_ID ORDER BY A.Asset_ID, C1.Description,C.Description,M1.Description")
'V.local.sSQL.Set("Select A.Asset_ID, A.TAG_ID, C1.Hw_Sw, C1.Description, C2.Description, M1.Description, M2.Description, V.Company_Name, A.Invoice, A.Serial_Number, A.Asset_Description, A.Cost, A.VID, A.Date_Received FROM AM_Asset as A, AM_Make as M1, AM_Make as M2, AM_Asset_Catergory as C1, AM_Asset_Catergory as C2, AM_Vendor_Details as V WHERE A.Asset_Category_ID = C1.Asset_Category_ID AND A.Make = M1.Asset_Make_ID AND A.Model = M2.Asset_Make_ID AND A.Subcategory=C2.Asset_Category_ID AND  A.VID = V.Vendor_ID ORDER BY A.ASSET_ID")
F.ODBC.Connection!con.OpenRecordsetRO("rst","Select * from AM_Asset Order By Asset_ID")
F.Intrinsic.Control.DoUntil(V.ODBC.con!rst.EOF,=,True)
	F.Intrinsic.Control.If(V.ODBC.con!rst.FieldVal!Tag_ID,<>," ")
		F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!Asset_ID,"*!*",V.Local.sText)
		F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!Tag_ID,"*!*",V.Local.sText)
		F.Intrinsic.String.Concat("select Hw_Sw, Description from AM_Asset_Catergory where Asset_Category_ID = ",V.ODBC.con!rst.FieldVal!Asset_Category_ID," ",V.Local.sSQL)
		F.ODBC.connection!con.OpenRecordsetRO("rstLu",V.Local.sSql)
		F.Intrinsic.Control.If(V.ODBC.con!rstLu.EOF,=,False)
			F.Intrinsic.Control.If(V.ODBC.con!rstLU.FieldVal!Hw_Sw,=,0)
				F.Intrinsic.String.Concat(V.Local.sText,"Hardware","*!*",V.Local.sText)
			F.Intrinsic.Control.Else
				F.Intrinsic.String.Concat(V.Local.sText,"Software","*!*",V.Local.sText)
			F.Intrinsic.Control.EndIf
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rstLu.FieldVal!Description,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!**!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		F.ODBC.con!rstLu.Close
		F.Intrinsic.String.Concat("select Description from AM_Asset_Catergory where Asset_Category_ID = ",V.ODBC.con!rst.FieldVal!subcategory," ",V.Local.sSql)
		F.ODBC.connection!con.OpenRecordsetRO("rstLu",V.Local.sSql)
		F.Intrinsic.Control.If(V.ODBC.con!rstLu.EOF,=,False)
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rstLu.FieldVal!Description,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		F.ODBC.con!rstLu.Close
		'Make
		'F.Intrinsic.UI.Msgbox(V.ODBC.con!rst.FieldVal!Make)
		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValisnull!Make,=,False)
	'		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldVal!Make,<>,"")
				F.Intrinsic.String.Concat("select Description from AM_Make where Asset_Make_ID = ",V.ODBC.con!rst.FieldVal!Make," ",V.Local.sSql)
				'F.Intrinsic.UI.Msgbox(V.Local.sSQL)
				F.ODBC.connection!con.OpenRecordsetRO("rstLu",V.Local.sSql)
				F.Intrinsic.Control.If(V.ODBC.con!rstLu.EOF,=,False)
					F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rstLu.FieldVal!Description,"*!*",V.Local.sText)
				F.Intrinsic.Control.Else
					F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
				F.Intrinsic.Control.EndIf
				F.ODBC.con!rstLu.Close
	'		F.Intrinsic.Control.EndIf
		F.Intrinsic.Control.else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.Endif

		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValisnull!Model,=,False)
			F.Intrinsic.String.Concat("select Description from AM_Make where Asset_Make_ID = ",V.ODBC.con!rst.FieldVal!Model," ",V.Local.sSql)
			F.ODBC.connection!con.OpenRecordsetRO("rstLu",V.Local.sSql)
			F.Intrinsic.Control.If(V.ODBC.con!rstLu.EOF,=,False)
				F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rstLu.FieldVal!Description,"*!*",V.Local.sText)
			F.Intrinsic.Control.Else
				F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
			F.Intrinsic.Control.EndIf
			F.ODBC.con!rstLu.Close
		F.Intrinsic.Control.Else
				F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		'company name - Vendor_Details
		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValisnull!VID,=,False)
			F.Intrinsic.String.Concat("select  Company_Name From AM_Vendor_Details Where Vendor_ID = ",V.ODBC.con!rst.FieldVal!VID," ",V.Local.sSQL)
			F.ODBC.Connection!con.OpenRecordsetRO("rstLu",V.Local.sSQL)
			F.Intrinsic.Control.If(V.ODBC.con!rstLu.EOF,=,False)
				F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rstLu.FieldVal!Company_Name,"*!*",V.Local.sText)
			F.Intrinsic.Control.Else
				F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
			F.Intrinsic.Control.EndIf
			F.ODBC.con!rstLu.Close
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		'Invoice
		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValIsNull!Invoice,=,False)
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!Invoice,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		'Serial_Number
		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValIsNull!Serial_Number,=,False)
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!Serial_Number,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		'Asset_Description
		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValIsNull!Asset_Description,=,False)
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!Asset_Description,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		'Cost
		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValIsNull!Cost,=,False)
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!Cost,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		'VID
		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValIsNull!VID,=,False)
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!VID,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		'Date_Received
		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValIsNull!Date_Received,=,False)
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!Date_Received,"@!@",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"@!@",V.Local.sText)
		F.Intrinsic.Control.EndIf
F.Intrinsic.Control.EndIf
F.ODBC.con!rst.MoveNext
F.Intrinsic.Control.Loop
F.ODBC.con!rst.Close
F.Intrinsic.Debug.Stop

F.Intrinsic.Math.Sub(V.Local.sText.Length,3,V.Local.iTemp)
F.Intrinsic.String.Left(V.Local.sText,V.Local.iTemp,V.Local.sText)
'F.Intrinsic.UI.Msgbox(V.Local.sText)
F.Intrinsic.UI.BrowserFromString("Select Asset",V.Local.sText,"*!*","@!@",V.Local.sTitle,V.Local.iWidth,9950,6000,V.Local.sRet)

F.intrinsic.control.If(V.Local.sRet,<>,"***CANCEL***")
	F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
	F.Intrinsic.String.Concat("Select * From AM_Asset Where Asset_ID = ",V.Local.sRet(0),"",V.Local.sSQL)
	F.ODBC.Connection!con.OpenRecordsetRO("rstAID",V.Local.sSQL)
	F.Intrinsic.Control.If(V.ODBC.con!rstAID.EOF,=,False)
	Gui.F_Asset.txtAsset_AssetLookup.Text(V.Local.sRet(0))
	Gui.F_Asset.txtAsset_TagID.Text(V.Local.sRet(1))
	Gui.F_Asset.dlstAsset_Type.Text(V.Local.sRet(2))
	Gui.F_Asset.dlstAsset_Category.Text(V.Local.sRet(3))
	Gui.F_Asset.dlstAsset_subcategory.Text(V.Local.sRet(4))
	Gui.F_Asset.dlstAsset_Make.Text(V.Local.sRet(5))
	Gui.F_Asset.dlstAsset_Model.Text(V.Local.sRet(6))
	Gui.F_Asset.txtAsset_Vendor.Text(V.Local.sRet(7))
	Gui.F_Asset.txtAsset_Invoice.Text(V.Local.sRet(8))
	Gui.F_Asset.txtAsset_Serial.Text(V.Local.sRet(9))
	Gui.F_Asset.txtAsset_Description.Text(V.Local.sRet(10))
	Gui.F_Asset.txtAsset_Cost.Text(V.Local.sRet(11))
	Gui.F_Asset.txtAsset_VID.Text(V.Local.sRet(12))
	'Try and put in date variable, declare the variable, then set it to the v.local.sret(13), then the dtp is set to the new variable not the sret
	V.Local.Datex.Set(V.Local.sRet(13))
	Gui.F_Asset.dtpAsset_DateReceived.value(V.Local.datex)
F.Intrinsic.Debug.Stop
	F.Intrinsic.Control.If(V.ODBC.con!rstAID.FieldVal!Status,=,1)
		Gui.F_Asset.dlstAsset_Status.Text("Active")
	F.Intrinsic.Control.ElseIf(V.ODBC.con!rstAID.FieldVal!Status,=,2)
		Gui.F_Asset.dlstAsset_Status.Text("Inactive")
	F.Intrinsic.Control.Else
		Gui.F_Asset.dlstAsset_Status.Text("Retired")
	F.Intrinsic.Control.EndIf

	F.Intrinsic.Control.EndIf
	F.ODBC.con!rstAID.Close
F.Intrinsic.Control.EndIf
F.Intrinsic.Debug.Stop

Program.Sub.Asset_Lookup.End

