		'Asset ID
		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValIsNull!Asset_ID,=,False)
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!Asset_ID,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
