F.Intrinsic.String.Split("Asset ID *!*Tag ID*!* Hw_Sw*!*Category*!*SubCategory*!*Make*!*Model","*!*",V.Local.sTitle)
F.Intrinsic.String.Split("1000*!*2000*!*0*!*2000*!*1500*!*1000*!*1500*!*0*!*0*!*0*!*0*!*0*!*0","*!*",V.Local.iWidth)
F.ODBC.Connection!con.OpenRecordsetRW("rst","Select * from AM_Asset Order By Asset_ID")
F.Intrinsic.Control.DoUntil(V.ODBC.con!rst.EOF,=,True)
	F.Intrinsic.Control.If(V.ODBC.con!rst.FieldVal!Tag_ID,<>," ")
		F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!Asset_ID,"*!*",V.Local.sText)
		F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!Tag_ID,"*!*",V.Local.sText)
		F.Intrinsic.String.Concat("select Hw_Sw, Description from AM_Asset_Catergory where Asset_Category_ID = ",V.ODBC.con!rst.FieldVal!Asset_Category_ID," ",V.Local.sSQL)
		F.ODBC.connection!con.OpenRecordsetRO("rstLu",V.Local.sSql)
		F.Intrinsic.Control.If(V.ODBC.con!rstLu.EOF,=,False)
			F.Intrinsic.Control.If(V.ODBC.con!rstLU.FieldVal!Hw_Sw,=,0)
				F.Intrinsic.String.Concat(V.Local.sText,"Hardware","*!*",V.Local.sText)
			F.Intrinsic.Control.Else
				F.Intrinsic.String.Concat(V.Local.sText,"Software","*!*",V.Local.sText)
			F.Intrinsic.Control.EndIf
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rstLu.FieldVal!Description,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!**!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		F.ODBC.con!rstLu.Close
		F.Intrinsic.String.Concat("select Description from AM_Asset_Catergory where Asset_Category_ID = ",V.ODBC.con!rst.FieldVal!subcategory," ",V.Local.sSql)
		F.ODBC.connection!con.OpenRecordsetRO("rstLu",V.Local.sSql)
		F.Intrinsic.Control.If(V.ODBC.con!rstLu.EOF,=,False)
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rstLu.FieldVal!Description,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		F.ODBC.con!rstLu.Close
		'Make
			F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValisnull!Make,=,False)
				F.Intrinsic.String.Concat("select Description from AM_Make where Asset_Make_ID = ",V.ODBC.con!rst.FieldVal!Make," ",V.Local.sSql)
				F.ODBC.connection!con.OpenRecordsetRO("rstLu",V.Local.sSql)
				F.Intrinsic.Control.If(V.ODBC.con!rstLu.EOF,=,False)
					F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rstLu.FieldVal!Description,"*!*",V.Local.sText)
				F.Intrinsic.Control.Else
					F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
				F.Intrinsic.Control.EndIf
				F.ODBC.con!rstLu.Close
		F.Intrinsic.Control.else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.Endif

		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValisnull!Model,=,False)
			F.Intrinsic.String.Concat("select Description from AM_Make where Asset_Make_ID = ",V.ODBC.con!rst.FieldVal!Model," ",V.Local.sSql)
			F.ODBC.connection!con.OpenRecordsetRO("rstLu",V.Local.sSql)
			F.Intrinsic.Control.If(V.ODBC.con!rstLu.EOF,=,False)
				F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rstLu.FieldVal!Description,"*!*",V.Local.sText)
			F.Intrinsic.Control.Else
				F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
			F.Intrinsic.Control.EndIf
			F.ODBC.con!rstLu.Close
		F.Intrinsic.Control.Else
				F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		'company name - Vendor_Details
'		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValisnull!VID,=,False)
'			F.Intrinsic.String.Concat("select  Company_Name From AM_Vendor_Details Where Vendor_ID = ",V.ODBC.con!rst.FieldVal!VID," ",V.Local.sSQL)
'			F.ODBC.Connection!con.OpenRecordsetRO("rstLu",V.Local.sSQL)
'			F.Intrinsic.Control.If(V.ODBC.con!rstLu.EOF,=,False)
'				F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rstLu.FieldVal!Company_Name,"*!*",V.Local.sText)
'			F.Intrinsic.Control.Else
'				F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
'			F.Intrinsic.Control.EndIf
'			F.ODBC.con!rstLu.Close
'		F.Intrinsic.Control.Else
'			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
'		F.Intrinsic.Control.EndIf
F.Intrinsic.Debug.Stop
		'MNotes
		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValisnull!MNotes,=,False)
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!MNotes,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		'Warrant End Date
		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValisnull!Warranty_End_DT,=,False)
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!Warranty_End_DT,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
		F.Intrinsic.Control.EndIf
		'Tag ID
		F.Intrinsic.Control.If(V.ODBC.con!rst.FieldValIsNull!Tag_ID,=,False)
			F.Intrinsic.String.Concat(V.Local.sText,V.ODBC.con!rst.FieldVal!Tag_ID,"*!*",V.Local.sText)
		F.Intrinsic.Control.Else
			F.Intrinsic.String.Concat(V.Local.sText,"*!*",V.Local.sText)
