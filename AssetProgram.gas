program.sub.cmdvendor_save_click.start
V.Local.iVID.Declare(Long)
V.Local.sSQL.Declare(String)
V.Local.sError.Declare(String)
V.Local.iRet.Declare(Long)

F.Intrinsic.Control.SetErrorHandler("cmdvendor_save_click_Err")
F.Intrinsic.Control.ClearErrors

F.Intrinsic.Control.If(V.Screen.F_Vendor!txtVendor_CompanyName.Text,=,"")
	F.Intrinsic.UI.Msgbox("Enter or Choose Vendor")
F.Intrinsic.Control.ElseIf(V.Screen.F_Vendor!txtVendor_AcctNum.Text,=,"")
	F.Intrinsic.UI.Msgbox("Enter the Account Number")
F.Intrinsic.Control.Elseif(V.Screen.F_Vendor!txtVendr_PhoneNum.Text,=,"")
	F.Intrinsic.UI.Msgbox("Enter vendor phone number")
F.Intrinsic.Control.Else
	F.Intrinsic.String.Instr(V.screen.F_Vendor!txtVendor_Email.Text,"@",1,V.Local.iRet)
	F.Intrinsic.Control.If(V.Local.iRet,=,0)
		F.Intrinsic.UI.Msgbox("Invalid Email - Please use @")
	F.Intrinsic.Control.Else
		F.Intrinsic.String.Instr(V.Screen.F_Vendor!txtVendor_Email.Text," ",V.Local.iRet)
		F.Intrinsic.Control.If(V.Local.iRet,<>,0)
			F.Intrinsic.UI.Msgbox("No Spaces allowed in email")
		F.Intrinsic.Control.Else

			F.Intrinsic.Control.If(V.Screen.F_Vendor!txtVendor_VID.Text,=,"")
				V.Local.iVID.Set(-1)
			F.Intrinsic.Control.Else
				V.Local.iVID.Set(V.Screen.F_Vendor!txtVendor_VID.Text,=,"")
			F.Intrinsic.Control.EndIf
F.Intrinsic.Debug.Stop
			F.Intrinsic.String.Concat("Select * From AM_Vendor_Details Where Vendor_ID = ",V.local.iVID,"",V.Local.sSQL)
			F.ODBC.Connection!con.OpenRecordsetRW("rstSave",V.Local.sSQL)
			F.Intrinsic.Control.If(V.ODBC.con!rstSave.EOF,=,True)
				F.ODBC.con!rstSave.AddNew
			F.Intrinsic.Control.EndIf
			F.ODBC.con!rstSave.Set!Company_Name(V.Screen.F_Vendor!txtVendor_CompanyName.Text)
			F.ODBC.con!rstSave.Set!Vendor_Contact_Name(V.screen.F_Vendor!txtVendor_ContactName.Text)
			F.ODBC.con!rstSave.Set!Account_Number(V.Screen.F_Vendor!txtVendor_AcctNum.Text)
			F.ODBC.con!rstSave.Set!Address(V.Screen.F_Vendor!txtVendor_Address.Text)
			F.ODBC.con!rstSave.Set!City(V.Screen.F_Vendor!txtVendor_City.Text)
			F.ODBC.con!rstSave.Set!State(V.Screen.F_Vendor!txtVendor_State.Text)
			F.ODBC.con!rstSave.Set!Zipcode(V.Screen.F_Vendor!txtVendor_Zipcode.Text)
			F.ODBC.con!rstSave.Set!Vendor_Phone_Number(V.Screen.F_Vendor!txtVendr_PhoneNum.Text)
			F.ODBC.con!rstSave.Set!Web_Address(V.Screen.F_Vendor!txtVendor_WebAddress.Text)
			F.ODBC.con!rstSave.Set!Email(V.Screen.F_Vendor!txtVendor_Email.Text)
			F.ODBC.con!rstSave.Set!Notes(V.Screen.F_Vendor!mltxtVendor_Notes.Text)
			F.ODBC.con!rstSave.Update
			F.ODBC.con!rstSave.Close
			F.Intrinsic.Control.CallSub(Cmdvendor_clear_click)
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.Endif
F.Intrinsic.Control.Endif

F.Intrinsic.Control.ExitSub

F.Intrinsic.Control.Label("cmdvendor_save_click_Err")
F.Intrinsic.Control.If(V.Ambient.ErrorNumber,<>,0)
    Function.Intrinsic.String.Concat("Error Occurred ",V.Ambient.ErrorNumber," with description ",V.Ambient.ErrorDescription,V.Local.sError)
'F.ODBC.con!rstSave.Close
    F.Intrinsic.UI.Msgbox(V.Local.sError)
Function.Intrinsic.Control.EndIf



