program.sub.cmdsearch_search_click.start
V.Local.iemp.Declare(Long)
V.Local.iC.Declare(Long)
V.Local.iU.Declare(Long)
V.Local.sSQL.Declare(String)

F.Intrinsic.Control.If(V.Screen.F_Search!txtSearch_EmployeeNum.Text,=,"")
	F.Intrinsic.UI.Msgbox("No Employee Selected")
F.Intrinsic.Control.Else

	Gui.F_Search.gsflxSrchList.Rows(2)
	Gui.F_Search.gsflxSrchList.Cols(5)
	Gui.F_Search.gsflxSrchList.TextMatrix(0,0,"Tag #")
	Gui.F_Search.gsflxSrchList.ColWidth(0,1000)
	Gui.F_Search.gsflxSrchList.TextMatrix(1,0,"Category")
	Gui.F_Search.gsflxSrchList.ColWidth(1,1000)
	Gui.F_Search.gsflxSrchList.TextMatrix(2,0,"Sub Category")
	Gui.F_Search.gsflxSrchList.ColWidth(2,1500)
	Gui.F_Search.gsflxSrchList.TextMatrix(3,0,"Make")
	Gui.F_Search.gsflxSrchList.ColWidth(3,1000)
	Gui.F_Search.gsflxSrchList.TextMatrix(4,0,"Model")
	Gui.F_Search.gsflxSrchList.ColWidth(4,1250)

	'Build Style and apply using for loop
	F.Intrinsic.Control.For(V.Local.iC,0,4,1)
		Gui.F_Search.gsflxSrchList.BuildStyle(1,V.Local.iC,"type","Locked","true")
		Gui.F_Search.gsflxSrchList.BuildStyle(1,V.Local.iC,"Format","alignment","4")
		'Gui.F_Search.gsflxSrchList.BuildStyle(1,V.Local.iC,"Color","Back",V.Color.Orange)
	F.Intrinsic.Control.Next(V.Local.iC)

	Gui.F_Search.gsflxSrchList.ApplyStyle(1,1)
'	F.Intrinsic.Debug.InvokeDebugger
'	F.Intrinsic.Debug.Stop
	V.uGlobal.uSrch.Redim(-1,-1)
	F.Intrinsic.String.Concat("Select Asset_ID, Tag_ID from AM_Asset_Assignment where Employee_ID= ",V.Screen.F_Search!txtSearch_EmpID.Text," order by AA_ID Desc ",V.Local.sSQL)
	'F.Intrinsic.UI.Msgbox(V.Local.sSQL)
	F.ODBC.Connection!con.OpenRecordsetRO("rstS",V.Local.sSQL)

	F.Intrinsic.Control.DoUntil(V.ODBC.con!rstS.EOF,=,True)
		F.Intrinsic.Control.If(V.ODBC.con!rstS.FieldValisnull!Tag_ID,=,False)
		
		F.Intrinsic.String.Concat("Select Top 1 Assign_Date, Employee_ID from AM_Asset_Assignment where Asset_ID=",V.odbc.con!rstS.FieldVal!Asset_ID," order by Assign_Date DESC ",V.Local.sSQL)
		'F.Intrinsic.UI.Msgbox(V.Local.sSQL)
		F.ODBC.Connection!con.OpenRecordsetRO("rstLU",V.Local.sSQL)
			F.Intrinsic.Control.If(V.ODBC.con!rstLu.EOF,=,False)
				V.Local.iemp.Set(V.Screen.F_Search!txtSearch_EmpID.Text)
				'V.Local.iemp.Set(V.Screen.F_Search!txtSearch_EmplD.Text)

				F.Intrinsic.Control.If(V.ODBC.con!rstLu.FieldVal!Employee_ID,=,V.Local.iemp)
					F.Intrinsic.Control.If(V.uGlobal.uSrch.UBound,=,-1)
						V.uGlobal.uSrch.Redim(0,0)
						V.Local.iU.Set(0)
					F.Intrinsic.Control.Else
						V.Local.iU.Set(V.uGlobal.uSrch.UBound)
						F.Intrinsic.Math.Add(V.Local.iU,1,V.Local.iU)
						V.uGlobal.uSrch.RedimPreserve(V.uGlobal.uSrch.LBound,V.Local.iU)
					F.Intrinsic.Control.EndIf
				F.Intrinsic.Control.EndIf
				F.Intrinsic.Control.If(V.Local.iU,<=,V.uGlobal.USrch.UBound)
					F.Intrinsic.String.Concat("select A.Tag_ID, A.Asset_ID, C1.Description as Category, C2.Description as SubCat, M1.Description as Make, M2.Description as Model From ((((AM_Asset as A Left JOIN AM_Asset_Catergory as C1 ON A.Asset_Category_ID = C1.Asset_Category_ID) LEFT JOIN AM_Asset_Catergory as C2 ON A.Subcategory = C2.Asset_Category_ID) LEFT JOIN AM_Make as M1 ON A.Make = M1.Asset_Make_ID) LEFT JOIN AM_Make as M2 ON A.Model = M2.Asset_Make_ID) where A.Asset_ID=",V.ODBC.con!rstS.FieldVal!Asset_ID,"",V.Local.sSQL)
					F.ODBC.Connection!con.OpenRecordsetRO("rstFLX",V.Local.sSQL)
					F.Intrinsic.Control.If(V.ODBC.con!rstFLX.EOF,=,False)
						V.uGlobal.USrch(v.Local.iU)!Tag_ID.Set(V.ODBC.con!rstFLX.FieldVal!Tag_ID)
						V.uGlobal.USrch(v.Local.iU)!Category.Set(V.ODBC.con!rstFLX.FieldVal!Category)
						V.uGlobal.USrch(v.Local.iU)!Subcategory.Set(V.ODBC.con!rstFLX.FieldVal!Subcat)
					F.Intrinsic.Control.If(V.ODBC.con!rstFLX.FieldValisnull!Make,=,False)
						V.uGlobal.USrch(v.Local.iU)!Make.Set(V.ODBC.con!rstFLX.FieldVal!Make)
					F.Intrinsic.Control.Else
						V.uGlobal.USrch(v.Local.iU)!Make.Set("")
					F.Intrinsic.Control.EndIf
					F.Intrinsic.Control.If(V.ODBC.con!rstFLX.FieldValisnull!Model,=,False)
							V.uGlobal.USrch(v.Local.iU)!Model.Set(V.ODBC.con!rstFLX.FieldVal!Model)
					F.Intrinsic.Control.Else
							V.uGlobal.USrch(v.Local.iU)!Model.Set("")
					F.Intrinsic.Control.EndIf
					F.Intrinsic.Control.EndIf
					F.ODBC.con!rstFLX.Close
				F.Intrinsic.Control.EndIf
			F.Intrinsic.Control.EndIf
			F.ODBC.con!rstLU.Close
		F.Intrinsic.Control.EndIf
		F.ODBC.con!rstS.MoveNext
	F.Intrinsic.Control.Loop
	F.ODBC.con!rstS.Close
	F.Intrinsic.Control.If(V.uGlobal.USrch.UBound,=,-1)
		F.Intrinsic.UI.Msgbox("Employee doesn't have assets assigned")
		F.Intrinsic.Control.Else
		Gui.F_Search.gsflxSrchList.LoadfromUDT("uGlobal.uSrch","Tag_ID*!*Category*!*Subcategory*!*Make*!*Model",1)
		Gui.F_Search.gsflxSrchList.FixedRows(1)
	F.Intrinsic.Control.Endif
F.Intrinsic.Control.EndIf

program.sub.cmdsearch_search_click.end

